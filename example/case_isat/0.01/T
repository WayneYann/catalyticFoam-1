FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0.01";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 573.149999999999977;

boundaryField
{
    reactingWall
    {
        type            catalyticWall;
        gradient        uniform 0;
    }
    inertWall
    {
        type            zeroGradient;
    }
    inlet
    {
        type            fixedValue;
        value           uniform 573.149999999999977;
    }
    outlet
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
}


